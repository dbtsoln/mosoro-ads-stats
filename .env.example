# VK Ads API - OAuth2 (recommended)
VK_ADS_CLIENT_ID=your_client_id_here
VK_ADS_CLIENT_SECRET=your_client_secret_here

# VK Ads API - Legacy token (deprecated, use OAuth2 instead)
# VK_ADS_API_TOKEN=your_token_here

# PostgreSQL
POSTGRES_URI=postgresql://user:password@host:port/database
POSTGRES_SSL=false

# Scheduler (interval in minutes)
FAST_STATS_INTERVAL=5
SUMMARY_STATS_INTERVAL=10
METADATA_UPDATE_INTERVAL=60

# Application
NODE_ENV=development
LOG_LEVEL=info
